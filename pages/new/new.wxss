/* pages/new/new.wxss */
.content{
  width: 100%;
  height: 100%;
  background-color: #f8f8f9;
  position: absolute;
}
.titimg{
  width: 100%;
  margin: 0 auto;
  height: 160rpx;

  
}
.titimg image{
  margin: 0 auto;
  width: 100%;
  height: 160rpx;

}
.text{
  background-color: #fff;
   width: 92%;
  margin: 0 auto;
  margin-top: 10rpx;
  border: 4rpx solid #f8f8f9;
  border-radius: 23rpx;
  box-shadow: 5rpx 5rpx 5rpx #e9eaec;
   margin-bottom: 60rpx;
  
}
.title{
  display: block;
  text-align: center;
  margin-top: 30rpx;
  line-height: 70rpx;
  font-family: 微软雅黑;
  font-size: 38rpx;
  color: #fac542
}
.pop{
  width: 85%;
  height: 100%;
  margin: 20rpx auto;
  margin-top: 30rpx;
  display: block;
   font-family: 微软雅黑;
  font-size: 28rpx;
  color: #495060;
  line-height: 45rpx;
}